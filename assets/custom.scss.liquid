  font-family: 'roc_groteskthin';
  src: url({{'roc_grotesk_thin-webfont.woff2' | asset_url}}) format('woff2'),
    url({{'roc_grotesk_thin-webfont.woff' | asset_url}}) format('woff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'roc_groteskregular';
  src: url({{'roc_grotesk_regular-webfont.woff2' | asset_url}}) format('woff2'),
    url({{'roc_grotesk_regular-webfont.woff' | asset_url}}) format('woff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'roc_groteskmedium';
  src: url({{'roc_grotesk_medium-webfont.woff2' | asset_url}}) format('woff2'),
    url({{'roc_grotesk_medium-webfont.woff' | asset_url}}) format('woff');
  font-weight: normal;
